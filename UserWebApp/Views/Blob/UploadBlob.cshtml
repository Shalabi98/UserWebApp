@model UserWebApp.Models.Student
@{
    Layout = "_Layout";
}

<form class="col-md-12" method="post" asp-action="UploadBlob" asp-controller="Blob" enctype="multipart/form-data">
    <div class="form-group">
        <input asp-for="Image" type="file" class="form-control" name="file" />
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-outline-success"><i class="fa fa-database"></i>&nbsp;Upload Image</button>
    </div>
</form>
